{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "DATABASE_URL": "postgresql://placeholder:placeholder@localhost:5432/placeholder",
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/manifest.json",
      "headers": {
        "Cache-Control": "public, max-age=3600, s-maxage=3600"
      }
    },
    {
      "src": "/icons/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=86400, s-maxage=86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 